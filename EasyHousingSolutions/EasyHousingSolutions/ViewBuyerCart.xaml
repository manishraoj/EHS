<Page x:Class="EasyHousingSolutions.ViewBuyerCart"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:EasyHousingSolutions"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="ViewBuyerCart" Background="Lavender">

    <Grid >
        <Button x:Name="btnLogOut" Content="LogOut" HorizontalAlignment="Right" Height="25" VerticalAlignment="Top" Margin="0,0,10,0" Click="btnLogOut_Click" ></Button>
        <Button x:Name="btnShow" Content="My Cart
                " HorizontalAlignment="Left" Margin="208,30,0,0" VerticalAlignment="Top" Width="82" Click="btnShow_Click"/>

        <WrapPanel x:Name="MyWrapPanel" Margin="0,37,0,0">
            <StackPanel Name="stackwrap"></StackPanel>
        </WrapPanel>
    </Grid>
</Page>
